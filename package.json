    "react-dom": "16.8.6",
    "react-native": "0.59.8",
    "react-native-animatable": "1.3.2",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "9.4.0",
    "react-native-web": "0.11.4",
    "svgs": "4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "flow-bin": "^0.100.0",
    "husky": "2.4.0",
    "lint-staged": "8.2.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "prettier": "1.18.0",
    "react-scripts": "3.0.1",
